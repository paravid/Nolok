        ["I"] = "Î™", ["i"] = "iã€ª",
        ["J"] = "J", ["j"] = "Ñ˜",
        ["K"] = "K", ["k"] = "k",
        ["L"] = "L", ["l"] = "Ó",
        ["M"] = "M", ["m"] = "m",
        ["N"] = "N", ["n"] = "â´–",
        ["O"] = "O", ["o"] = "o",
        ["P"] = "Ð ", ["p"] = "p",
        ["Q"] = "Q", ["q"] = "q",
        ["R"] = "R", ["r"] = "r",
        ["S"] = "S", ["s"] = "à¸£",
