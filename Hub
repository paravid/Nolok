        ["U"] = "U", ["u"] = "uã€ª",
        ["V"] = "V", ["v"] = "v",
        ["W"] = "W", ["w"] = "w",
        ["X"] = "X", ["x"] = "x",
        ["Y"] = "Y", ["y"] = "y",
        ["Z"] = "Z", ["z"] = "z",
        [" "] = "",
        }
    
function transformName(input)
    local transformed = ""
    for i = 1, #input do
        local char = input:sub(i, i) 
        transformed = transformed .. (lettersss[char] or char) 
    end
    return transformed
end
main:CreateBox("Reason", "string", function(AutoReason)
    AutoReasonss = transformName(AutoReason)
end)

