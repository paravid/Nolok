function onCharacterDeath()
    reanimated = false
    local character = plr.Character or plr.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")

    if humanoidRootPart then
        respawnPosition = humanoidRootPart.CFrame -- Copy the current position
    end
    Message("Character has died!", "Reanimation reset", 5)
end

function onCharacterAdded(character)
    -- Connect the Died event for the new character
    character:WaitForChild("Humanoid").Died:Connect(onCharacterDeath)

    -- Teleport the player to the saved position after respawn
    if respawnPosition then
        character:WaitForChild("HumanoidRootPart").CFrame = respawnPosition
        wait(1)
        respawnPosition = nil

    end
end

-- Connect to the CharacterAdded event
plr.CharacterAdded:Connect(onCharacterAdded)

-- If the character already exists, connect to its Died event
if plr.Character then
    onCharacterAdded(plr.Character)
end

Playerr:CreateDropdown("HaxterFelix Animations", HaxterFelix, function(selectedHax)
    mode = selectedHax
    print("Selected Animation Mode: " .. mode)
end)

Playerr:CreateDropdown("Shavine Animations", Shavine, function(ShavinesModes)
    mode = ShavinesModes
    print("Selected Animation Mode: " .. mode)
end)


Playerr:CreateDropdown("Random Animations", Random, function(RandomModes)
    mode = RandomModes
    print("Selected Animation Mode: " .. mode)
end)

Playerr:CreateToggle("Fly", function(EnableFly)
    if EnableFly then
        if not IsOnMobile then
            flying = true
            local KeyDownFunction, KeyUpFunction

            KeyDownFunction = mouse.KeyDown:Connect(function(key)
                if flying then
                    if key:lower() == "w" then
                        ctrl.f = 1
                        PlayAnim(10714177846, 4.65, 0) 
                    elseif key:lower() == "s" then
                        ctrl.b = -1
                        PlayAnim(10147823318, 4.11, 0) 
                    elseif key:lower() == "a" then
                        ctrl.l = -1
                        PlayAnim(10147823318, 3.55, 0) 
                    elseif key:lower() == "d" then
                        ctrl.r = 1
                        PlayAnim(10147823318, 4.81, 0) 
                    end
                end
            end)

            KeyUpFunction = mouse.KeyUp:Connect(function(key)
                if flying then
                    if key:lower() == "w" then
                        ctrl.f = 0
                    elseif key:lower() == "s" then
                        ctrl.b = 0
                    elseif key:lower() == "a" then
                        ctrl.l = 0
                    elseif key:lower() == "d" then
                        ctrl.r = 0
                    end
                    StopAllAnims() 
                    if ctrl.f + ctrl.b + ctrl.l + ctrl.r == 0 then
                        PlayAnim(10714347256, 4, 0) 
                    end
                end
            end)

            Fly()
        else
            mobilefly(plr)
        end
    else
        flying = false
        unmobilefly(plr)
        if KeyDownFunction then KeyDownFunction:Disconnect() end
        if KeyUpFunction then KeyUpFunction:Disconnect() end
        StopAllAnims() 
        ctrl = {f = 0, b = 0, l = 0, r = 0} 
    end
end)

Playerr:CreateBox("Fly Speed", "string", function(FlySpeed) 
    FlySpeedS = (FlySpeed)
end)


Playerr:CreateToggle("Auto Glitch", function(AutoGlitchss) 
        getgenv().AutoGlitchs = AutoGlitchss
        if not getgenv().AutoGlitchs then 		_G.AutoGlitchsss = false
            return end
            _G.AutoGlitchsss = true
            
            AutoGlitchssss = RunService.Stepped:Connect(function()
                task.spawn(function()
                        if game.Workspace.RoundSettings.VotingTime.Value == 3 then
plr.Character.Head:Destroy()
wait(1)
                        end

            end)
        end)

            repeat task.wait() until _G.AutoGlitchsss == false
            AutoGlitchssss:Disconnect()
        end)

        local RunService = game:GetService("RunService")
        local Players = game:GetService("Players")
        local plr = Players.LocalPlayer
        
        local antiflingEnabled = false
        local checkInterval = 1 
        local lastCheckTime = 0
        
        
        local originalProperties = {}
        
        
        function applyAntiFling()
            for _, p in pairs(Players:GetPlayers()) do
                if p ~= plr and p.Character then
                    for _, v in pairs(p.Character:GetChildren()) do
                        if v:IsA("BasePart") or (v:IsA("Accessory") and v:FindFirstChild("Handle")) then
                            local part = v:IsA("Accessory") and v.Handle or v
                            
                            if not originalProperties[part] then
                                originalProperties[part] = {
                                    CanCollide = part.CanCollide,
                                    Massless = part.Massless
                                }
                            end
                            
                            part.CanCollide = false
                            part.Velocity = Vector3.new(0, 0, 0)
                            part.RotVelocity = Vector3.new(0, 0, 0)
                            part.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
                            part.Massless = true
                        end
                    end
                end
            end
        end
        
        
        function restoreProperties()
            for part, properties in pairs(originalProperties) do
                if part and properties then
                    if part:IsA("BasePart") or (part:IsA("Accessory") and part:FindFirstChild("Handle")) then
                        part.CanCollide = properties.CanCollide
                        part.Massless = properties.Massless
                    end
                end
            end
            
            originalProperties = {}
        end
        
        RunService.Heartbeat:Connect(function(dt)
            if antiflingEnabled then
                lastCheckTime = lastCheckTime + dt
                if lastCheckTime >= checkInterval then
                    lastCheckTime = 0
                    applyAntiFling()
                end
            else
                restoreProperties()
            end
        end)
        
        
        Playerr:CreateToggle("AntiFling", function(antiflinggg)
            antiflingEnabled = antiflinggg
        end)
        
