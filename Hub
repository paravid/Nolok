    local lettersss = {
        ["A"] = "Ð", ["a"] = "à¸¥",
        ["B"] = "B", ["b"] = "b",
        ["C"] = "C", ["c"] = "á²ƒ",
        ["D"] = "D", ["d"] = "d",
