                    LeftShoulder.C0 = LeftShoulder.C0:Lerp(cf(-1, 0.5, 0) * euler(0, -1.5707963267948966 + 0.3490658503988659 * sin((sine + 0.1) * 8), 0.005235987755982988 + 0.6981317007977318 * sin(sine * 8) * fw), deltaTime) 
