    "Haxter Loader ABCDEFGH()",
    "AKADMINABCDEFGH()"
}

local modifiedMessages = {}
for _, message in ipairs(messagesToDetect) do
    table.insert(modifiedMessages, modifyString(message))
end
local Players = game:GetService("Players")

function attachNametagToHeadHaxterHub(character, player, messageType)
    local head = character:WaitForChild("Head", 5)
    if not head then return end

    -- Remove existing nametag if it exists
    local existingTag = game.CoreGui:FindFirstChild(player.Name .. "Haxter")
    if existingTag then  existingTag:Destroy() end

    -- Create a new BillboardGui for the nametag
    local tag = Instance.new("BillboardGui")
    tag.Adornee = head
    tag.Active = true
    tag.Name = player.Name .. "Haxter"
    tag.Size = UDim2.new(0, 100, 0, 50) -- Adjust size as needed
    tag.StudsOffset = Vector3.new(0, 6, 0) -- Offset above the head
    tag.AlwaysOnTop = true
    tag.MaxDistance = math.huge

    -- Create a container for the username
    local container = Instance.new("TextButton")
    container.Size = UDim2.new(1, 0, 1, 0)
    container.BackgroundColor3 = Color3.fromRGB(128, 0, 128) -- Purple color
    container.BackgroundTransparency = 0.6 -- Set transparency
    container.BorderSizePixel = 0
    container.Text = ""
    container.Parent = tag

    -- Username label
    local userLabel = Instance.new("TextLabel")
    userLabel.Size = UDim2.new(1, 0, 1, 0)
    userLabel.BackgroundTransparency = 0.6 -- Set transparency
    userLabel.TextColor3 = Color3.new(1, 1, 1) -- White text for better readability
    userLabel.TextSize = 14
    userLabel.Font = Enum.Font.GothamBold
    userLabel.TextXAlignment = Enum.TextXAlignment.Center
    userLabel.TextScaled = true
    userLabel.Parent = container

        userLabel.Text = "HaxterHub User: " .. player.Name

    -- Click handler for teleport
    container.MouseButton1Click:Connect(function()
        if player ~= Players.LocalPlayer then
            teleportToPlayer(player)
        end
    end)

    -- Use a while true do loop for scaling
    tag.Parent = game.CoreGui
    while tag.Parent do
        wait(0.1) -- Adjust the wait time as needed
        if not (character and character:FindFirstChild("Head") and Players.LocalPlayer.Character) then
            break
        end
        local localHead = Players.LocalPlayer.Character:FindFirstChild("Head")
        if not localHead then return end
        local distance = (head.Position - localHead.Position).Magnitude
        local scale = math.clamp(1 - (distance / 100), 0.5, 1.5) -- Adjust scale range as needed
        tag.Size = UDim2.new(0, 100 * scale, 0, 50 * scale)
    end

    -- Cleanup when the tag is removed
    tag.AncestryChanged:Connect(function(_, parent)
        if not parent then
            tag:Destroy() -- Ensure the tag is destroyed
        end 
    end)
end

function attachNametagToHeadAKADMIN(character, player, messageType)
    local head = character:WaitForChild("Head", 5)
    if not head then return end

    -- Remove existing nametag if it exists
    local existingTag = game.CoreGui:FindFirstChild(player.Name .. "AK")
    if existingTag then  existingTag:Destroy() end

    -- Create a new BillboardGui for the nametag
    local tag = Instance.new("BillboardGui")
    tag.Adornee = head
    tag.Active = true
    tag.Name = player.Name .. "AK"
    tag.Size = UDim2.new(0, 100, 0, 50) -- Adjust size as needed
    tag.StudsOffset = Vector3.new(0, 12, 0) -- Offset above the head
    tag.AlwaysOnTop = true
    tag.MaxDistance = math.huge

    -- Create a container for the username
    local container = Instance.new("TextButton")
    container.Size = UDim2.new(1, 0, 1, 0)
    container.BackgroundColor3 = Color3.fromRGB(128, 0, 128) -- Purple color
    container.BackgroundTransparency = 0.6 -- Set transparency
    container.BorderSizePixel = 0
    container.Text = ""
    container.Parent = tag

    -- Username label
    local userLabel = Instance.new("TextLabel")
    userLabel.Size = UDim2.new(1, 0, 1, 0)
    userLabel.BackgroundTransparency = 0.6 -- Set transparency
    userLabel.TextColor3 = Color3.new(1, 1, 1) -- White text for better readability
    userLabel.TextSize = 14
    userLabel.Font = Enum.Font.GothamBold
    userLabel.TextXAlignment = Enum.TextXAlignment.Center
    userLabel.TextScaled = true
    userLabel.Parent = container

        userLabel.Text = "AK User: " .. player.Name

    -- Click handler for teleport
    container.MouseButton1Click:Connect(function()
        if player ~= Players.LocalPlayer then
            teleportToPlayer(player)
        end
    end)

    -- Use a while true do loop for scaling
    tag.Parent = game.CoreGui
    while tag.Parent do
        wait(0.1) -- Adjust the wait time as needed
        if not (character and character:FindFirstChild("Head") and Players.LocalPlayer.Character) then
            break
        end
        local localHead = Players.LocalPlayer.Character:FindFirstChild("Head")
        if not localHead then return end
        local distance = (head.Position - localHead.Position).Magnitude
        local scale = math.clamp(1 - (distance / 100), 0.5, 1.5) -- Adjust scale range as needed
        tag.Size = UDim2.new(0, 100 * scale, 0, 50 * scale)
    end

    -- Cleanup when the tag is removed
    tag.AncestryChanged:Connect(function(_, parent)
        if not parent then
            tag:Destroy() -- Ensure the tag is destroyed
        end 
    end)
end


function teleportToPlayer(player)
    local character = player.Character
    if character and character:FindFirstChild("HumanoidRootPart") then
        Players.LocalPlayer.Character.HumanoidRootPart.CFrame = character.HumanoidRootPart.CFrame
    end
end



